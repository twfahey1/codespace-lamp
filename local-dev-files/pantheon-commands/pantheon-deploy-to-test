SITE_NAME=$(php <<"EOF"
<?php
$site_data = yaml_parse_file(__DIR__ . "/.devcontainer/local-dev-files/site-config.yml");
echo $site_data['site_name'];
EOF
)
SITE_ENV=$(php <<"EOF"
<?php
$site_data = yaml_parse_file(__DIR__ . "/.devcontainer/local-dev-files/site-config.yml");
echo $site_data['default_environment'];
EOF
)
terminus env:deploy $SITE_NAME.test --note="${@:1}"